"#[doc =\n\" Common interface for `PSP22Votes`, and other `Votes`-enabled contracts.\"]\npub trait Votes\n{\n    #[doc = \" The amount of votes owned by `account`.\"] #[ink(message)]\n    #[ink(selector = 826414179u32)] fn get_votes(& self, account : AccountId)\n    -> Balance ;\n    #[doc =\n    \" The amount of votes delegated to `account` at the time `timestamp`.\"]\n    #[ink(message)] #[ink(selector = 3530804688u32)] fn\n    get_past_votes(& self, account : AccountId, timestamp : Timestamp) ->\n    Result < Balance, GovernanceError > ;\n    #[doc = \" The total amount of votes at the time `timestamp`.\"]\n    #[ink(message)] #[ink(selector = 4130306626u32)] fn\n    get_past_total_supply(& self, timestamp : Timestamp) -> Result < Balance,\n    GovernanceError > ;\n    #[doc = \" Returns the address delegated to by `delegator`.\"]\n    #[ink(message)] #[ink(selector = 1997709268u32)] fn\n    delegates(& mut self, delegator : AccountId) -> Option < AccountId > ;\n    #[doc = \" Delegate votes from `signer` to `delegatee`.\"] #[ink(message)]\n    #[ink(selector = 2356841741u32)] fn\n    delegate(& mut self, delegatee : AccountId) -> Result < (),\n    GovernanceError > ;\n    #[doc = \" Delegate votes from `signer` to `delegatee` using a signature.\"]\n    #[ink(message)] #[ink(selector = 775733394u32)] fn\n    delegate_by_signature(& mut self, signer : AccountId, delegatee :\n    AccountId, nonce : u64, expiry : Timestamp, signature : Signature,) ->\n    Result < (), GovernanceError > ;\n}"